FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    top
    {
    type            totalPressure;
    gamma           1.4;
    p0              uniform 10e5;
    }
    
    bottom
    {
        type            fixedFluxPressure;
    }
    
    right
    {
        type            fixedFluxPressure;
    }
    
    back
    {
        type            wedge;
    }

    front
    {
        type            wedge;
    }

    axis
    {
        type            empty;
    }
}

// ************************************************************************* //
